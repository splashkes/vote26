        {
          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "WITH funnel_steps AS (\n  SELECT \n    'QR Discovery' as step,\n    1 as step_order,\n    COUNT(DISTINCT person_id) as unique_users,\n    COUNT(*) as total_events\n  FROM people_qr_scans \n  WHERE created_at >= NOW() - INTERVAL '${__range_s:regex} seconds'\n  \n  UNION ALL\n  \n  SELECT \n    'Event Registration',\n    2,\n    COUNT(DISTINCT person_id),\n    COUNT(*)\n  FROM event_registrations\n  WHERE created_at >= NOW() - INTERVAL '${__range_s:regex} seconds'\n  \n  UNION ALL\n  \n  SELECT \n    'Voting Participation',\n    3, \n    COUNT(DISTINCT person_id),\n    COUNT(*)\n  FROM votes\n  WHERE created_at >= NOW() - INTERVAL '${__range_s:regex} seconds'\n  \n  UNION ALL\n  \n  SELECT \n    'Auction Bidding',\n    4,\n    COUNT(DISTINCT person_id),\n    COUNT(*)\n  FROM bids \n  WHERE created_at >= NOW() - INTERVAL '${__range_s:regex} seconds'\n  \n  UNION ALL\n  \n  SELECT \n    'Payment Completed',\n    5,\n    COUNT(DISTINCT person_id),\n    COUNT(*)\n  FROM payment_processing\n  WHERE status IN ('completed', 'succeeded')\n    AND created_at >= NOW() - INTERVAL '${__range_s:regex} seconds'\n),\nfunnel_with_rates AS (\n  SELECT \n    fs.*,\n    LAG(fs.unique_users) OVER (ORDER BY fs.step_order) as prev_step_users\n  FROM funnel_steps fs\n)\nSELECT \n  step,\n  unique_users,\n  total_events,\n  CASE \n    WHEN prev_step_users > 0 THEN \n      ROUND((unique_users::numeric / prev_step_users * 100), 2)\n    ELSE 100.0\n  END as conversion_rate_percent,\n  CASE \n    WHEN prev_step_users > 0 THEN \n      (prev_step_users - unique_users)\n    ELSE 0\n  END as users_lost,\n  CASE \n    WHEN prev_step_users > 0 THEN \n      ROUND(((prev_step_users - unique_users)::numeric / prev_step_users * 100), 2)\n    ELSE 0.0\n  END as drop_off_rate_percent\nFROM funnel_with_rates\nORDER BY step_order;",
          "refId": "A",
          "select": [
            [
              {
                "params": [
                  "value"
                ],
                "type": "column"
              }
            ]
          ],
          "timeColumn": "time",
          "where": [
