          "rawQuery": true,
          "rawSql": "-- Key Conversion Rates as Gauges\nSELECT \n  'Voting to Bidding' as metric,\n  CASE \n    WHEN voters > 0 THEN ROUND((bidders::numeric / voters * 100), 2)\n    ELSE 0\n  END as conversion_rate\nFROM (\n  SELECT \n    (SELECT COUNT(DISTINCT person_id) FROM votes WHERE created_at >= $__timeFrom() AND created_at <= $__timeTo()) as voters,\n    (SELECT COUNT(DISTINCT person_id) FROM bids WHERE created_at >= $__timeFrom() AND created_at <= $__timeTo()) as bidders\n) rates\n\nUNION ALL\n\nSELECT \n  'Bidding to Payment' as metric,\n  CASE \n    WHEN bidders > 0 THEN ROUND((payers::numeric / bidders * 100), 2)\n    ELSE 0\n  END as conversion_rate\nFROM (\n  SELECT \n    (SELECT COUNT(DISTINCT person_id) FROM bids WHERE created_at >= $__timeFrom() AND created_at <= $__timeTo()) as bidders,\n    (SELECT COUNT(DISTINCT person_id) FROM payment_processing WHERE status IN ('completed', 'succeeded') AND created_at >= $__timeFrom() AND created_at <= $__timeTo()) as payers\n) rates\n\nUNION ALL\n\nSELECT \n  'Payment Success Rate' as metric,\n  CASE \n    WHEN total_attempts > 0 THEN ROUND((successful::numeric / total_attempts * 100), 2)\n    ELSE 0\n  END as conversion_rate\nFROM (\n  SELECT \n    (SELECT COUNT(*) FROM payment_processing WHERE created_at >= $__timeFrom() AND created_at <= $__timeTo()) as total_attempts,\n    (SELECT COUNT(*) FROM payment_processing WHERE status IN ('completed', 'succeeded') AND created_at >= $__timeFrom() AND created_at <= $__timeTo()) as successful\n) rates;",
