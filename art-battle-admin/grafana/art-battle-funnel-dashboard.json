        {
          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "-- Per-event funnel performance\nSELECT \n  e.name as event_name,\n  c.name as city,\n  co.name as country,\n  e.event_start_datetime,\n  COUNT(DISTINCT v.person_id) as voters,\n  COUNT(DISTINCT b.person_id) as bidders,\n  COUNT(DISTINCT p.person_id) as buyers,\n  COUNT(DISTINCT b.id) as total_bids,\n  COUNT(DISTINCT p.id) as total_payments,\n  ROUND(COALESCE(SUM(p.amount), 0), 2) as revenue,\n  CASE \n    WHEN COUNT(DISTINCT v.person_id) > 0 THEN \n      ROUND((COUNT(DISTINCT b.person_id)::numeric / COUNT(DISTINCT v.person_id) * 100), 2)\n    ELSE 0\n  END as voting_to_bidding_rate,\n  CASE \n    WHEN COUNT(DISTINCT b.person_id) > 0 THEN \n      ROUND((COUNT(DISTINCT p.person_id)::numeric / COUNT(DISTINCT b.person_id) * 100), 2)\n    ELSE 0\n  END as bidding_to_payment_rate\nFROM events e\nJOIN cities c ON e.city_id = c.id\nJOIN countries co ON c.country_id = co.id\nLEFT JOIN votes v ON e.id = v.event_id AND v.created_at >= $__timeFrom() AND v.created_at <= $__timeTo()\nLEFT JOIN bids b ON e.id = b.event_id AND b.created_at >= $__timeFrom() AND b.created_at <= $__timeTo()\nLEFT JOIN payment_processing p ON e.id = p.event_id AND p.created_at >= $__timeFrom() AND p.created_at <= $__timeTo() AND p.status IN ('completed', 'succeeded')\nWHERE e.event_start_datetime >= $__timeFrom()::date - INTERVAL '30 days'\nGROUP BY e.id, e.name, c.name, co.name, e.event_start_datetime\nHAVING COUNT(DISTINCT v.person_id) > 0 OR COUNT(DISTINCT b.person_id) > 0\nORDER BY revenue DESC, voters DESC;",
          "refId": "A",
          "select": [
